{
  "name": "lightstudiesweb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix-dry-run",
    "format": "prettier src",
    "format:fix": "prettier src --write",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "singleQuote": false
  },
  "eslintConfig": {
    "ignorePatterns": [
      "coverage/**",
      "node_modules/**",
      "public/*.html",
      "!src/**"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {}
  },
  "jest": {
    "coveragePathIgnorePatterns": [
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
